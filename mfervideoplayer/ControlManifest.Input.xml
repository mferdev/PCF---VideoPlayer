      <domain>https://www.youtube.com</domain>
    </external-service-usage>
    <property name="url" display-name-key="YoutubeUrl" description-key="Url de un video de youtube que visualizaremos en un reproductor de video" of-type="SingleLine.URL" usage="bound" required="true" />
    <property name="width" display-name-key="Ancho" description-key="Dimensiones del reproductor de video" of-type="Whole.None" usage="input" required="false" />
    <property name="height" display-name-key="Altura" description-key="Dimensiones del reproductor de video" of-type="Whole.None" usage="input" required="false" />
    <resources>
      <code path="index.ts" order="1"/>
    </resources>
  </control>
</manifest>
